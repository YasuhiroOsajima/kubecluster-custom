# If not one master, there are made HA cluster.
# If you make HA master cluster, you have to specify 3 or more master node.
[master]
kube-master1 mngip=192.168.66.22
kube-master2 mngip=192.168.66.23
kube-master3 mngip=192.168.66.24

[minion]
kube-minion1 mngip=192.168.66.25

[loadbalancer]
kube-minion1 mngip=192.168.66.25

[servers:children]
master
minion
loadbalancer

[cluster:children]
master
minion
loadbalancer
